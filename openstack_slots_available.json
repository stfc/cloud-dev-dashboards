      "pluginVersion": "10.3.3",
      "targets": [
        {
          "alias": "$tag_flavor",
          "datasource": {
            "type": "influxdb",
            "uid": "openstack_grafana"
          },
          "groupBy": [
            {
              "params": [
                "flavor"
              ],
              "type": "tag"
            }
          ],
          "measurement": "SlotsAvailable",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "SELECT last(\"SlotsAvailable\") FROM \"SlotsAvailable\" WHERE (\"instance\" =~  /^$Instance$/  AND \"flavor\" !~ /.*test.*/ AND \"flavor\" !~ /.*temp.*/ AND \"flavor\" =~ /^l.*/) AND $timeFilter GROUP BY \"flavor\"",
          "rawQuery": true,
          "refId": "A",
          "resultFormat": "time_series",
          "select": [
            [
              {
                "params": [
                  "SlotsAvailable"
                ],
                "type": "field"
              },
              {
                "params": [],
                "type": "last"
              }
            ]
          ],
          "tags": [
            {
              "key": "flavor",
              "operator": "!~",
              "value": "/.*test.*/"
            },
            {
              "condition": "AND",
              "key": "flavor",
              "operator": "!~",
              "value": "/.*temp.*/"
            },
            {
              "condition": "AND",
              "key": "flavor",
              "operator": "=~",
              "value": "/^l.*/"
            }
          ]
        }
      ],
      "title": "L* flavors",
      "type": "stat"
    },
